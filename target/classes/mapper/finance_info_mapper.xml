<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="mybatis.sql.finance_info_mapper">

	<select id = "Finance_InfoList"  resultType="Finance_InfoVO" >		
		SELECT *
		FROM finance_info
		LIMIT 100
	</select>
	
	<select id = "Alarm_FinanceList"  resultType="java.lang.String"  parameterType="java.lang.String">		
		SELECT symbol
		FROM finance_info
		WHERE payment_date = #{date}
	</select>
</mapper>
 		